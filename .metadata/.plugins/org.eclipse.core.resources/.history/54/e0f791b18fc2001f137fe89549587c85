package entities;

import enums.Color;

public class Triangle extends Shapewithcolor implements Shape {

	private double height;
	private double width;

	public Triangle(Color color, double height, double width) {
		super(color);
		this.height = height;
		this.width = width;
	}

	@Override
	public double area() {
		return (width * height) / 2;
	}

}
