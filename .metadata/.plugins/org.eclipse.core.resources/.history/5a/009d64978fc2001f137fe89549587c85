package entities;

public class Triangle extends Shapewithcolor implements Shape {

	private double height;
	private double width;
	
	public Triangle(double height, double width, Color color) {
		super(color);
		this.height = height;
		this.width = width;
	}

	@Override
	public double area() {
		return (width * height) / 2;
	}

}
