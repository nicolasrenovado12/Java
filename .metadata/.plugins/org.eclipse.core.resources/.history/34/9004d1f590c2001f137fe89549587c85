package entities;

import enums.Color;

public class Triangle implements Shape extends Shapewithcolor  {

	private double height;
	private double width;

	public Triangle(double height, double width, Color color) {
		super();
		this.height = height;
		this.width = width;
		this.color = color;
	}

	@Override
	public double area() {
		return (width * height) / 2;
	}

}
